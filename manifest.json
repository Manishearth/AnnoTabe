{
  "manifest_version": 2,

  "name": "AnnoTabe",
  "description": "Lets users annotate windows",
  "version": "1.2",

  "permissions": [
    "tabs",
    "storage"
  ],
   "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icontildal.png",
    "default_popup": "popup.html"
  }
}
