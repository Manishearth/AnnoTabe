{
  "manifest_version": 2,

  "name": "AnnoTabe",
  "description": "Leave annotations on tabs you want to get back to later",
  "version": "1.3.2",

  "permissions": [
    "tabs",
    "storage"
  ],
   "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {"19":"icontildal19.png","38":"icontildal19.png"},
    "default_popup": "popup.html"
  },
  "icons": { "16": "icontildalactive16.png",
           "48": "icontildalactive48.png",
          "128": "icontildalactive128.png" },
  "incognito" : "spanning"
}
